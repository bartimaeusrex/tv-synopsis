html
  head
    title TV Synopsis
    link(rel="stylesheet" href="/css/styles.css")
body
  h1= showdata.title
      h2 Developed by:
        ul
          li= showdata.developedBy
      h2 Starring:
        ul
          li= showdata.starring
      h2 Seasons:
        ul
          li
            a(href=`/showdata/${showdata.seasons}`)= showdata.seasons[title]